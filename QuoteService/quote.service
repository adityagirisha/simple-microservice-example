[Unit]
Description=Quote Service Application
After=network.target

[Service]
User=root
WorkingDirectory=~/simple-microservice-example/QuoteService/
ExecStart=~/simple-microservice-example/QuoteService/venv/bin/python ~/simple-microservice-example/QuoteService/main.py
Restart=always

[Install]
WantedBy=multi-user.target
